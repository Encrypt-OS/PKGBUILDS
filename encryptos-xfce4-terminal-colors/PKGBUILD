pkgname=Encrypt-OS-xfce4-terminal-colors
pkgver=4
pkgrel=1
pkgdesc="Encrypt-OS colorscheme for xfce4-terminal"
arch=('any')
depends=('xfce4-terminal')
url="https://Encrypt-OS.com/"
license=('GPL3')
source=("${pkgname}-${pkgver}.tar.gz::https://github.com/Encrypt-OS/${pkgname}/archive/${pkgver}.tar.gz")
md5sums=('8589ed78873a5f6a45a9580d319dc3cc')

package() {
  cd "$srcdir/Encrypt-OS-xfce4-terminal-colors-${pkgver}"
  install -Dm 644 Encrypt-OS.theme \
    "$pkgdir/usr/share/xfce4/terminal/colorschemes/Encrypt-OS.theme"
}
